<launch>
  <arg name="machine" default="c1" />

  <include file="$(find pr2_machine)/pr2.machine" />

  <rosparam file="/etc/ros/robot.yaml"/>

  <node pkg="app_manager" type="appmaster" name="appmaster"
        args="-p 11312"
        machine="$(arg machine)" />
  <node pkg="app_manager" type="app_manager" name="app_manager"
        args="--applist $(find jsk_pr2_startup)/apps/app_list"
        output="screen" machine="$(arg machine)">
    <param name="interface_master" value="http://localhost:11312"/>
  </node>

  <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch" />
</launch>
